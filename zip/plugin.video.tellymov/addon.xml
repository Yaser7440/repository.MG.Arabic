<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="plugin.video.tellymov"
  version="1.0.8"
  name="Tellymov"
  provider-name="Made by TSmedia Team">
   <requires>
        <import addon="xbmc.python" version="2.1.0" optional="true" />
		
        <import addon="script.module.urlresolver" version="1.0.0"/>
		<import addon="script.module.addon.common" version="2.0.0" />
		<import addon="script.module.requests"  version="2.9.1"/>
  </requires>
 
  <extension point="xbmc.python.pluginsource"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>tellmov.tv</summary>
    <description> MOVIES plugin, Arabic , Hollywood, European and Bollywood movies from tellmov</description>
   
    <platform>all</platform>
  </extension>
</addon>
